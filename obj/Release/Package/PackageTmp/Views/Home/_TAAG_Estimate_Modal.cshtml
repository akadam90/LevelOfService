@model LOS.Models.TAAG_Estimate_Form_Model

<div class="modal fade" tabindex="-1" role="dialog" id="tag-estimate-modal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Preliminary TAAG Estimate & CPM</h4>
            </div>
            <div class="modal-body">
                <p id="taag_estimate_note"><i>Note: For the most accurate TAAG Resource estimate and CPM, the TAAG New Business Questionnaire must be completed. All TAAG Resource estimates and CPM's  presented here must be confirmed by the TAAG Team prior to sharing with the Client. </i></p>
                </br>
                <div class="container">
                    <div class="row">
                        <div class="col-sm-5">Select your approximate GROSS billings:</div>
                        <div class="col-sm-4">
                            <select id="input-taag-billings" class="form-control">
                                <option id="input-taag-billings-0" value="0">---</option>
                            @for (var i = 0; i < @Model.grossBillings.Count; i++)
                            {
                                <option id="input-taag-billings-@Model.grossBillings[i].Id" value="@Model.grossBillings[i].Id">@Model.grossBillings[i].Spending</option>
                            }
                            </select>
                        </div>
                    </div>
                    <br/>
                    <div class="row">
                        <div class="col-sm-2"></div>
                        <div class="col-sm-5"><label for="taag-total" class="control-label">FTE Allocation Total: </label></div>
                        <div class="col-sm-3"><input type="text" class="form-control underline-text" id="taag-total" name="" /></div>
                        <div class="col-sm-2"></div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3">
                            <label for="taag-director" class="control-label">Director</label>
                        </div>
                        <div class="col-sm-3">
                            <input type="text" class="form-control underline-text" id="taag-director" name="" />
                        </div>
                        <div class="col-sm-3">
                            <label for="taag-supervisor-tech-ops" class="control-label">Supervisor Tech Ops</label>
                        </div>
                        <div class="col-sm-3">
                            <input type="text" class="form-control underline-text" id="taag-supervisor-tech-ops" name="" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3">
                            <label for="taag-associate-director" class="control-label">Associate Director</label>
                        </div>
                        <div class="col-sm-3">
                            <input type="text" class="form-control underline-text" id="taag-associate-director" name="" />
                        </div>
                        <div class="col-sm-3">
                            <label for="taag-associate-analyst" class="control-label">Associate Analyst</label>
                        </div>
                        <div class="col-sm-3">
                            <input type="text" class="form-control underline-text" id="taag-associate-analyst" name="" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3">
                            <label for="taag-supervisor-analyst" class="control-label">Supervisor Analyst</label>
                        </div>
                        <div class="col-sm-3">
                            <input type="text" class="form-control underline-text" id="taag-supervisor-analyst" name="" />
                        </div>
                        <div class="col-sm-3">
                            <label for="taag-associate-tech-ops" class="control-label">Associate Tech Ops</label>
                        </div>
                        <div class="col-sm-3">
                            <input type="text" class="form-control underline-text" id="taag-associate-tech-ops" name="" />
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-sm-3">
                            <label for="taag-cpm" class="control-label">Recommended CPM</label>
                        </div>
                        <div class="col-sm-3">
                            <input type="text" class="form-control underline-text" id="taag-cpm" name="" />
                        </div>
                        <div class="col-sm-3">
                            <label for="taag-service-level" class="control-label">Level of Service</label>
                        </div>
                        <div class="col-sm-3">
                            <input type="text" class="form-control underline-text" id="taag-service-level" name="" />
                        </div>

                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Save</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
